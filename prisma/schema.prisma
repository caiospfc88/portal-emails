generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Envio {
  id               Int      @id @default(autoincrement())
  id_email_enviado String   @unique
  nome             String?
  email_destino    String
  email_origem     String
  data_envio       DateTime
  assunto          String
  html             String? @db.LongText
  ultimo_evento    String
  template         String?
  agendado         DateTime?
  retornar_para    String?
}
